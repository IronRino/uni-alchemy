class Random {
    
    field int rand;

    function void setSeed(int seed) {
        let rand = seed;
    }

    function int mod(int dividend, int divider) {
        var int remainder;
        let remainder = dividend - ((dividend / divider) * divider);
        return remainder;
    }

    function int next(int max) {
        var int result;
        let result = mod(rand, max);
        return result;
    }
}